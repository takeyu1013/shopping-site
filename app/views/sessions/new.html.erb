<% provide(:title, "ログイン") %>
<h1>ログイン</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(url: login_path, scope: :session, local: true) do |form| %>
      <%= form.label :email %>
      <%= form.email_filed :email, class: 'form-control' %>
      <%= form.label :password %>
      <%= form.password_filed :password, class: 'form-control' %>

  </div>
</div>
